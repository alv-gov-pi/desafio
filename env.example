POSTGRES_DB=         # Nome do banco de dados PostgreSQL (ex.: dialogos_db)
POSTGRES_USER=       # Usuário do PostgreSQL (ex.: dialogos_user)
POSTGRES_PASSWORD=   # Senha do usuário PostgreSQL (utilize uma senha forte)
POSTGRES_HOST=       # Endereço do host do PostgreSQL (ex.: localhost ou nome do container)
POSTGRES_PORT=       # Porta de conexão do PostgreSQL (padrão: 5432)

DJANGO_SUPERUSER_USERNAME=  # Nome de usuário para o superusuário Django (use um nome único e seguro)
DJANGO_SUPERUSER_PASSWORD=  # Senha para o superusuário Django (deve ser forte; evite senhas comuns)
DJANGO_SUPERUSER_EMAIL=     # E-mail para o superusuário Django (utilize um e-mail válido para recuperação)
CREATE_SUPERUSER=       # Defina 1 para criar o superusuário automaticamente na implantação (use 0 em produção)